{
    "name": "jit/fleetrunnr",
    "description": "This is my package fleetrunnr",
    "keywords": [
        "jit",
        "laravel",
        "fleetrunnr"
    ],
    "homepage": "https://github.com/jit/fleetrunnr",
    "license": "MIT",
    "authors": [
        {
            "name": "jit",
            "email": "m.saliba@joinjit.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "guzzlehttp/guzzle": "^7.3",
        "illuminate/contracts": "^8.37",
        "spatie/laravel-package-tools": "^1.4.3"
    },
    "require-dev": {
        "nunomaduro/collision": "^5.3",
        "orchestra/testbench": "^6.15",
        "pestphp/pest": "^1.18",
        "pestphp/pest-plugin-laravel": "^1.1",
        "spatie/laravel-ray": "^1.23",
        "vimeo/psalm": "^4.8"
    },
    "autoload": {
        "psr-4": {
            "Jit\\Fleetrunnr\\": "src",
            "Jit\\Fleetrunnr\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jit\\Fleetrunnr\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "./vendor/bin/pest --no-coverage",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jit\\Fleetrunnr\\FleetrunnrServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
